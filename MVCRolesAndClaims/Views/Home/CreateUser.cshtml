@using MVCRolesAndClaims.ViewModels;
@model UserViewModel

@{
    ViewData["Title"] = "Create User";
}

<h1>Create new role</h1>

<div class="row">
    <div class="col-md-12">
        <form method="post">
            <div asp-validation-summary="All"></div>
            <div class="form-control-lg">
                <label asp-for="Email"></label>
                <input asp-for="Email" name="RoleName" autocomplete="off" spellcheck="false" />
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>
            <br />
            <button style="width:auto" type="submit" class="btn btn-primary">Create Email</button>
        </form>
    </div>
</div>

